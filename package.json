{
  "name": "slidev-theme-comfy",
  "version": "0.2.0",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "build:ci": "slidev build example.md --base /slidev-theme-comfy/",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "@catppuccin/palette": "^0.1.6",
    "@fontsource/atkinson-hyperlegible": "^4.5.11",
    "@fontsource/mononoki": "^4.5.3",
    "@fontsource/rammetto-one": "^4.5.11",
    "@slidev/types": "^0.40.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@iconify-json/noto": "^1.1.11",
    "@iconify-json/noto-v1": "^1.1.7",
    "@iconify-json/openmoji": "^1.1.22",
    "@slidev/cli": "^0.40.3",
    "@types/node": "^18.14.0",
    "eslint": "^8.34.0",
    "typescript": "^4.9.5"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "shiki",
    "defaults": {
      "fonts": {
        "serif": "Rammetto One",
        "sans": "Atkinson Hyperlegible",
        "mono": "Mononoki"
      }
    }
  }
}
